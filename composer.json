{
	"name": "pint/news-page",
	"description": "Add a page type for the news.",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Eric Chavaillaz",
			"email": "eric.chavaillaz@gmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "eric.chavaillaz@gmail.com",
		"issues": "https://github.com/echavaillaz/news_page/issues",
		"source": "https://github.com/echavaillaz/news_page"
	},
	"require": {
		"ext-pdo": "*",
		"georgringer/news": "^8.5",
		"php": "^7.4",
		"typo3/cms-core": "^10.4"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.0",
		"helmich/typo3-typoscript-lint": "^2.4",
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"Pint\\NewsPage\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "news_page"
		}
	}
